<?xml version="1.0" encoding="UTF-8"?>
		<xform>
			<declare prefix="layout">
				<column name="id" caption="ID号" mode="string" editable="false"/>
				<column name="operatorName" caption="操作人" mode="string" editable="false"/>
				<column name="operatorIP" caption="操作人IP" mode="string" editable="false"/>
                <column name="operationCode" caption="操作类型" mode="string" editable="false"/>
                <column name="operateTable" caption="操作的表" mode="string" editable="false"/>
				<column name="operateTime" caption="操作时间" mode="string" editable="false"/>
                <column name="content" caption="操作内容" mode="string"/>
			</declare>
			<layout>
				<TR>
					<TD width="50"><label binding="id"/></TD>
					<TD><input binding="id"/></TD>
				</TR>
				<TR>
					<TD><label binding="operatorName"/></TD>
					<TD><input binding="operatorName"/></TD>
				</TR>
				<TR>
					<TD><label binding="operateTime"/></TD>
					<TD><input binding="operateTime"/></TD>
				</TR>
				<TR>
					<TD valign="top"><label binding="content"/></TD>
					<TD><textarea binding="content" style="width:100%;height:100" readOnly="true"/></TD>
				</TR>
			</layout>
			<data>
			</data>
		</xform>
