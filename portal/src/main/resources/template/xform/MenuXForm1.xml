<?xml version="1.0" encoding="UTF-8"?>
<xform>
	<declare prefix="navigator">
		<column name="id" caption="ID号" mode="string" editable="false"/>
		<column name="name" caption="名称" mode="string" maxLength="50" empty="false"/>			
		<column name="description" caption="备注" mode="string"/>		
		<column name="parentId" caption="" mode="hidden"/>
		<column name="portalId" caption="" mode="hidden"/>
	</declare>
	<layout>
		<TR>
			<TD width="50"><label binding="id"/></TD>
			<TD><input binding="id" type="text"/></TD>
		</TR>
		<TR>
			<TD width="50"><label binding="name"/></TD>
			<TD><input binding="name" type="text"/></TD>
		</TR>
		<TR>
			<TD width="50"><label binding="description"/></TD>
			<TD><textarea binding="description" style="width:100%;height:100"/></TD>
		</TR>
	</layout>
	<data>
	</data>
</xform>
