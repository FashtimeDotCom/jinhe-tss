<?xml version="1.0" encoding="UTF-8"?>
<xform>
	<declare prefix="resourceType">
		<column name="resourceTypeId" caption="资源类型ID" mode="string" empty="false"/>
		<column name="rootId" caption="根节点ID" mode="number" inputReg="/^\-?\d*$/" empty="false"/>
		<column name="name" caption="名称" mode="string" empty="false"/>
		<column name="description" caption="描述" mode="string"/>
		
		<column name="unSuppliedTable" caption="未补全的表名" mode="string" empty="false"/>
		<column name="suppliedTable" caption="补全的表名" mode="string" empty="false"/>
		<column name="resourceTable" caption="资源的表名" mode="string" empty="false"/>
 
	</declare>
	<layout>
		<TR>
			<TD width="100"><label binding="resourceTypeId"/></TD>
			<TD><input binding="resourceTypeId"/></TD>
		</TR>
		<TR>
			<TD width="100"><label binding="rootId"/></TD>
			<TD><input binding="rootId"/></TD>
		</TR>
		<TR>
			<TD width="100"><label binding="name"/></TD>
			<TD><input binding="name"/></TD>
		</TR>
		<TR>
			<TD width="100"><label binding="description"/></TD>
			<TD><input binding="description" style="width:350;"/></TD>
		</TR>
		<TR>
			<TD width="100"><label binding="unSuppliedTable"/></TD>
			<TD><input binding="unSuppliedTable" style="width:350;"/></TD>
		</TR>
		<TR>
			<TD width="100"><label binding="suppliedTable"/></TD>
			<TD><input binding="suppliedTable" style="width:350;"/></TD>
		</TR>
		<TR>
			<TD width="100"><label binding="resourceTable"/></TD>
			<TD><input binding="resourceTable" style="width:350;"/></TD>
		</TR>
	</layout>
	<data>
	</data>
</xform>