<?xml version="1.0" encoding="UTF-8"?><Response><ChannelInfo>
<xform>
<declare>
	<column name="name" caption="名称" mode="string" empty="false"/>
	<column name="remark" caption="站点描述" mode="string" maxLength="255"/>
	<column name="overdueDate" caption="过期时间" mode="string" editor="comboedit" editorvalue="0|1|2|3|4|5" editortext="一年|六个月|一个季度|一个月|一个星期|一天"/>
	<column name="path" caption="发布路径" mode="string" empty="false"/>
	<column name="docPath" caption="附件路径" mode="string"/>
	<column name="imagePath" caption="图片路径" mode="string"/>	
</declare>
<layout>
	<TR>
		<TD width="50">
			<label binding="name"/>
		</TD>
		<TD colspan="3">
			<input binding="name"/>
		</TD>
		<TD width="50">
			<label binding="overdueDate"/>
		</TD>
		<TD>
			<input binding="overdueDate"/>
		</TD>
	</TR>
	<TR>
		<TD width="50">
			<label binding="path"/>
		</TD>
		<TD>
			<input binding="path"/>
		</TD>
		<TD width="50">
			<label binding="imagePath"/>
		</TD>
		<TD>
			<input binding="imagePath"/>
		</TD>
		<TD width="50">
			<label binding="docPath"/>
		</TD>
		<TD>
			<input binding="docPath"/>
		</TD>
	</TR>
	<TR>
		<TD width="50">
			<label binding="remark"/>
		</TD>
		<TD colspan="5">
			<textarea binding="remark" style="width:100%;height:100"/>
		</TD>
	</TR>
</layout>		
<data><row><imagePath><![CDATA[img]]></imagePath><docPath><![CDATA[doc]]></docPath><name><![CDATA[TSS门户]]></name><overdueDate><![CDATA[5]]></overdueDate><remark><![CDATA[asdasddfg萨斯]]></remark></row></data></xform></ChannelInfo></Response>
